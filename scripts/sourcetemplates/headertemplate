#ifndef ${PROJECTNAMEUPPER}_${NAMESPACEUPPER}_${CLASSNAMEUPPER}_H
#define ${PROJECTNAMEUPPER}_${NAMESPACEUPPER}_${CLASSNAMEUPPER}_H

//std inc
#include <iosfwd>

namespace ${PROJECTNAME}
{
	namespace ${NAMESPACE}
	{
		/*!
		 No description provided for ${CLASSNAME}
    	 */
		class ${CLASSNAME}
		{
			friend std::ostream& operator<< (std::ostream&, const ${NAMESPACE}::${CLASSNAME}&);
		
		public:
			// Mutating operators
			${CLASSNAME}& operator=(const ${CLASSNAME}&) = default;
			
			// Constructors, destructors
			${CLASSNAME}() = default;
			${CLASSNAME}(const ${CLASSNAME}&) = default;
			~${CLASSNAME}() = default;
			
		};

		std::ostream& operator<< (std::ostream&, const ${NAMESPACE}::${CLASSNAME}&);
        
	}

}

#endif /* ${PROJECTNAMEUPPER}_${NAMESPACEUPPER}_${CLASSNAMEUPPER}_H  */
